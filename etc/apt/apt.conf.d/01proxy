# Check it with the following.
#$ apt-config dump |grep -i proxy

#Use the following commands to check the configuration when the socks5 proxy is running and not running respectively.
#$ sudo apt-get update 

# Method 1:
#Acquire::http::proxy::deepin-wine.i-m.dev "socks5h://127.0.0.1:18887";
#Acquire::http::proxy::dl.google.com "socks5h://127.0.0.1:18887";

#Acquire::https::proxy::deepin-wine.i-m.dev "socks5h://127.0.0.1:18887";
#Acquire::https::proxy::dl.google.com "socks5h://127.0.0.1:18887";


# Method 2:
Acquire::http::proxy "socks5h://127.0.0.1:18887" {
     deepin-wine.i-m.dev;
     dl.google.com;
};
Acquire::https::proxy "socks5h://127.0.0.1:18887" {
     deepin-wine.i-m.dev;
     dl.google.com; 
};

Acquire::http::Proxy {
    mirrors.tuna.tsinghua.edu.cn DIRECT; 
};
Acquire::https::Proxy {
    mirrors.tuna.tsinghua.edu.cn DIRECT;    
};




